{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./src/**/*.tsx --localSchemaFile=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "autoprefixer": "^10.2.3",
    "graphql": "^15.5.0",
    "next": "10.0.5",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "17.0.1",
    "react-icon": "^1.0.0",
    "react-icons": "^4.1.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "apollo": "^2.32.1",
    "typescript": "^4.1.3"
  }
}
